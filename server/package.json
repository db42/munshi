{
    "name": "munshi-server",
    "version": "1.0.0",
    "description": "Backend server for Munshi tax filing application",
    "main": "dist/index.js",
    "scripts": {
        "types:generate": "ts-node src/scripts/generate-itr-types.ts",
        "types:post-process": "ts-node src/scripts/post-process-types.ts",
        "types:all": "npm run types:generate && npm run types:post-process",
        "db:init": "npx ts-node src/scripts/setupDb.ts",
        "start": "npm run db:init && node dist/index.js",
        "dev": "npm run db:init && nodemon",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "test:itr": "ts-node src/tests/eval-itr-test.ts",
        "test:itr:baseline": "ts-node src/tests/eval-itr-test.ts --create-baseline"
    },
    "engines": {
        "node": ">=22.0.0",
        "npm": ">=10.0.0"
    },
    "dependencies": {
        "@google/generative-ai": "^0.21.0",
        "@types/multer": "^1.4.12",
        "@types/uuid": "^10.0.0",
        "ajv": "^8.17.1",
        "ajv-draft-04": "^1.0.0",
        "axios": "^1.6.0",
        "cors": "^2.8.5",
        "csv-parse": "^5.6.0",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "pdf-parse": "^1.1.1",
        "pdfjs-dist": "^4.9.155",
        "pg": "^8.13.1",
        "pg-pool": "^3.6.1",
        "quicktype-core": "^23.0.170",
        "tabula-js": "^1.0.1",
        "uuid": "^11.0.3",
        "winston": "^3.8.2",
        "xlsx": "https://cdn.sheetjs.com/xlsx-0.20.3/xlsx-0.20.3.tgz"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/lodash": "^4.17.16",
        "@types/node": "^22.0.0",
        "@types/pdf-parse": "^1.1.4",
        "@types/pg": "^8.11.10",
        "@typescript-eslint/eslint-plugin": "^8.0.0-alpha.22",
        "@typescript-eslint/parser": "^8.0.0-alpha.22",
        "eslint": "^9.0.0",
        "jest": "^29.5.0",
        "jest-diff": "^30.0.0-beta.3",
        "nodemon": "^3.1.10",
        "prettier": "^2.8.7",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "ts-morph": "^22.0.0",
        "typescript": "^5.4.5"
    }
}
