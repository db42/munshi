{
    "name": "munshi-server",
    "version": "1.0.0",
    "description": "Backend server for Munshi tax filing application",
    "main": "dist/index.js",
    "scripts": {
        "db:init": "npx ts-node src/scripts/setupDb.ts",
        "start": "npm run db:init && node dist/index.js",
        "dev": "npm run db:init && nodemon",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest"
    },
    "dependencies": {
        "@types/multer": "^1.4.12",
        "@types/uuid": "^10.0.0",
        "cors": "^2.8.5",
        "dotenv": "^16.0.3",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "pg": "^8.13.1",
        "pg-pool": "^3.6.1",
        "uuid": "^11.0.3",
        "winston": "^3.8.2"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.11",
        "@types/pg": "^8.11.10",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "eslint": "^8.37.0",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.7",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.3"
    }
}
