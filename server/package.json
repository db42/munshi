{
    "name": "munshi-server",
    "version": "1.0.0",
    "description": "Backend server for Munshi tax filing application",
    "main": "dist/index.js",
    "scripts": {
        "generate-itr-types": "ts-node src/scripts/generate-itr-types.ts",
        "db:init": "npx ts-node src/scripts/setupDb.ts",
        "start": "npm run db:init && node dist/index.js",
        "dev": "npm run db:init && nodemon",
        "build": "tsc",
        "lint": "eslint . --ext .ts",
        "format": "prettier --write \"src/**/*.ts\"",
        "test": "jest",
        "test:itr": "node src/tests/eval-itr-test.js"
    },
    "dependencies": {
        "@google/generative-ai": "^0.21.0",
        "@types/multer": "^1.4.12",
        "@types/uuid": "^10.0.0",
        "ajv": "^8.17.1",
        "ajv-draft-04": "^1.0.0",
        "ajv-formats": "^3.0.1",
        "axios": "^1.6.0",
        "cors": "^2.8.5",
        "csv-parse": "^5.6.0",
        "dotenv": "^16.4.7",
        "express": "^4.18.2",
        "multer": "^1.4.5-lts.1",
        "pdf-parse": "^1.1.1",
        "pdfjs-dist": "^4.9.155",
        "pg": "^8.13.1",
        "pg-pool": "^3.6.1",
        "quicktype-core": "^23.0.170",
        "tabula-js": "^1.0.1",
        "uuid": "^11.0.3",
        "winston": "^3.8.2",
        "xlsx": "^0.18.5"
    },
    "devDependencies": {
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/jest": "^29.5.0",
        "@types/lodash": "^4.17.16",
        "@types/node": "^18.15.11",
        "@types/pdf-parse": "^1.1.4",
        "@types/pg": "^8.11.10",
        "@typescript-eslint/eslint-plugin": "^5.57.1",
        "@typescript-eslint/parser": "^5.57.1",
        "eslint": "^8.37.0",
        "jest": "^29.5.0",
        "nodemon": "^2.0.22",
        "prettier": "^2.8.7",
        "ts-jest": "^29.1.0",
        "ts-node": "^10.9.1",
        "typescript": "^5.0.3"
    }
}
